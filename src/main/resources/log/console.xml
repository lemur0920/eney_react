<?xml version="1.0" encoding="UTF-8"?>
<!-- <appender name="다른 파일이나 위치에서 해당 appender를 참조하는 이름" class="해당 클래스를 사용하여 콘솔에 로깅한다고 명시">-->
<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <!--<charset>UTF-8</charset>-->
        <pattern>[%d{yy.MM.dd HH:mm:ss}] [%thread] [%-5p] [%t %C:%L ] - %m%n</pattern>
    </encoder>
</appender>